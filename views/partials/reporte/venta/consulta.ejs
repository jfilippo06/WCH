                    <input type="text" class="form-control" placeholder="Tamaño" name="size">
                </div>
                <div class="col-md-1">
                    <input type="text" class="form-control" placeholder="Pagina" name="page">
                </div>
                <div class="col-md-2">
                    <input type="text" class="form-control" placeholder="N° Factura" name="numero">
                </div>
                <div class="col-md-2">
                    <input type="date" name="" id="">
                </div>
                <button type="submit" class="btn btn-primary col-md-2">Consultar</button>
            </form>
            
            <%- include('../../message/alert') %>
            <%- include('../../message/success') %>

        </div>
    </div>
</div>