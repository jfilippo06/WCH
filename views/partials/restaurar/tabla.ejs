<div class="col-lg-12">
    <div class="card">

        <div class="card-body">
            <h5 class="card-title">lista de respaldos</h5>
            <%- include('../message/alert') %>
            <%- include('../message/success') %>
            <table class="table table-bordered border-primary">
                <thead>
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">Nombre</th>
                        <th scope="col">fecha</th>
                        <th scope="col">Restaurar</th>
                    </tr>
                </thead>
                <tbody>
                    <% for(let i=0; i < link.length; i++) { %>
                        <tr>
                            <td><%= link[i].id %></td>
                            <td><%= link[i].name %></td>
                            <td><%= link[i].createdAt %></td>
                            <td>
                                <a href="/mantenimiento/paste/<%= link[i].id %>" class="btn btn-warning bi bi-cloud-arrow-up-fill"></a>
                            </td>
                        </tr>
                    <% } %>
                </tbody>
            </table>
            <div class="text-center">
                <% if (prev) { %>
                    <a class="btn btn-primary bi-arrow-left" href="<%= prev %>"></a>
                <% } %>
                <% if (next) { %>
                    <a class="btn btn-primary bi-arrow-right" href="<%= next %>"></a>
                <% } %>
            </div>
        </div>
    </div>
</div>