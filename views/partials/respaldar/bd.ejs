<div class="col-lg-12">
    <div class="card">
        <div class="card-body">
            <h5 class="card-title">Base de dato</h5>

            <%- include('../message/alert') %>
            <%- include('../message/success') %>

            <form action="/mantenimiento/copy" method="post">
                <input type="hidden" name="_csrf" value="<%= csrfToken %>">
                <div class="row mb-3">
                    <label for="inputEmail3" class="col-sm-2 col-form-label">Nombre:</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" id="link" name="link" required>
                    </div>
                </div>
                <div class="text-center">
                    <input class="btn btn-primary" type="submit" value="Respaldar Base de datos">
                </div>
            </form>

        </div>
    </div>
</div>